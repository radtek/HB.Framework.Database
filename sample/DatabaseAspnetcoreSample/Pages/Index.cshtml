@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Click to write, Refresh to read</h1>
    
    <form method="POST">
        <input type="submit" />
    </form>
    
    @foreach (PublisherEntity publisher in Model.Publishers)
    {
        <div>
            <span>@publisher.Guid</span>
            <span>@publisher.Version</span>
            <span>@publisher.Name</span>
        </div>
    }
</div>
